var REG_NONE = NewRegistrar('none', 'NONE');  // No registrar needed, as you're just managing DNS records
var DNS_NAMECHEAP = NewDnsProvider('namecheap', 'NAMECHEAP');

D('mihon.tools', REG_NONE, DnsProvider(DNS_NAMECHEAP),
    CNAME('backup', 'tachibk.netlify.app.'),
    CNAME('local', 'mihonlocal.netlify.app.')
);
